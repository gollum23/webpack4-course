{
  "name": "webpack4",
  "version": "1.0.0",
  "description": "Curso de Webpack 2019",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:multi": "webpack --config ./MultiEntryPoint/webpack.config.js --mode development",
    "build:css": "webpack --config ./cssStyleLoader/webpack.config.js --mode development",
    "build:plugins": "webpack --config ./plugins/webpack.config.js --mode development",
    "build:dev": "webpack-dev-server --config ./WebpackDevServer/webpack.config.js --mode development",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js --mode development",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js --mode development"
  },
  "keywords": [
    "Platzi",
    "Webpack"
  ],
  "author": "@Gollum23",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.8.0",
    "style-loader": "1.0.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.1"
  },
  "dependencies": {
    "@babel/runtime": "7.6.2",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  }
}
