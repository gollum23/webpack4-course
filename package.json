{
  "name": "webpack4",
  "version": "1.0.0",
  "description": "Curso de Webpack 2019",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:multi": "webpack --config ./MultiEntryPoint/webpack.config.js --mode development",
    "build:css": "webpack --config ./cssStyleLoader/webpack.config.js --mode development",
    "build:plugins": "webpack --config ./plugins/webpack.config.js --mode development",
    "build:dev": "webpack-dev-server --config ./WebpackDevServer/webpack.config.js --mode development",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js --mode development",
    "build:react": "webpack-dev-server --config ./react/webpack.config.js --mode development",
    "build:url": "webpack-dev-server --config ./urlLoader/webpack.config.js --mode development",
    "build:prepro": "webpack-dev-server --config ./prepro/webpack.config.js --mode development",
    "build:duplicated": "webpack --config ./preventDuplication/webpack.config.js --mode development",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:fast": "webpack --config ./dll/webpack.config.js",
    "build:dynamic:dll": "webpack --config ./dynamicImports/webpack.dll.config.js",
    "build:dynamic:fast": "webpack --config ./dynamicImports/webpack.config.js"
  },
  "keywords": [
    "Platzi",
    "Webpack"
  ],
  "author": "@Gollum23",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.2.0",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "less": "3.10.3",
    "less-loader": "5.0.0",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "url-loader": "2.1.0",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.8.1"
  },
  "dependencies": {
    "@babel/runtime": "7.6.2",
    "react": "16.9.0",
    "react-dom": "16.9.0"
  }
}
